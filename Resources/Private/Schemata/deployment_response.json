{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "properties": {
    "artifact_filename": {
      "type": "string",
      "pattern": "^pagedoctor-[a-z0-9]((-{0,2})[a-z0-9.]+)*"
    },
    "checksum": {
      "type": "string",
      "minLength": 64,
      "maxLength": 64
    }
  },
  "required": [
    "artifact_filename",
    "checksum"
  ]
}