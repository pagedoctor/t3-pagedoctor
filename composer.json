{
  "name": "pagedoctor/t3-pagedoctor",
  "type": "typo3-cms-extension",
  "description": "Pagedoctor - Rapidly build and deploy content elements for TYPO3 CMS without one line of code.",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "TYPO3 CMS",
    "Content Elements",
    "Pagedoctor",
    "Deployment"
  ],
  "require": {
    "../../vendor/typo3/cms-core": "^11.0 || ^12.0",
    "justinrainbow/json-schema": "^5.0"
  },
  "homepage": "https://www.pagedoctor.de",
  "support": {
    "source": "https://github.com/pagedoctor/t3-pagedoctor",
    "docs": "https://www.pagedoctor.de/docs/einfuehrung",
    "issues": "https://github.com/pagedoctor/t3-pagedoctor/issues"
  },
  "authors": [
    {
      "name": "Colin Atkins",
      "role": "Developer",
      "homepage": "https://www.pagedoctor.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Atkins\\Pagedoctor\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "pagedoctor"
    }
  }
}